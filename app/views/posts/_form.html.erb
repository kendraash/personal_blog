<%= render "layouts/errors", :object => @post %>

<%= form_for [@tag, @post] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :author %>
  <%= f.text_field :author %>
  <%= f.label :body %>
  
  <%= f.text_area :body, size: "60x120" %>
  <br>
  <%= f.submit  %>
<% end %>
